@model List<_200395854ass1.Models.Car>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>
<h2>Cars List</h2>


<table>
    <tr>
        <th>Id</th>
        <th>Branch</th>
        <th>Model</th>
        <th>Type</th>
        <th>Year</th>
    </tr>
    <tbody>
        @foreach (var car in Model)
        {
            <tr>
                <td>@car.Id</td>
                <td>@car.Brand</td>
                <td>@car.Model</td>
                <td>@car.Type</td>
                <td>@car.Year</td>
            </tr>
        }
    </tbody>
</table>





